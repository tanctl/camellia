circuit ComprehensiveDemo {
  inputs: public_sum, public_product, target_hash
  private inputs: secret_a, secret_b, nonce
  
  computed_sum = secret_a + secret_b
  computed_product = secret_a * secret_b
  privacy_hash = poseidon(secret_a, secret_b, nonce)
  
  assert computed_sum == public_sum
  assert computed_product == public_product  
  assert privacy_hash == target_hash
}